{
  "$schema": "http://json-schema.org/schema",
  "$id": "setup-seo",
  "title": "Setup Ngaox-Seo in your project",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The targeted project name",
      "$default": {
        "$source": "projectName"
      }
    },
    "module": {
      "type": "string",
      "description": "The module that will import the generated Seo-Module",
      "$default": {
        "$source": "moduleName"
      }
    }
  },
  "required": []
}
