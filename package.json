    "lint-staged": "^11.0.1",
    "ng-packagr": "^12.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.34.2",
    "typescript": "~4.3.5",
    "webpack-cli": "^4.8.0",
    "webpack-shebang-plugin": "^1.1.6",
    "yargs": "^17.1.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,json,css,scss,md,ts,html}": [
      "prettier --write",
      "git add"
