    "@types/jasmine": "^3.7.7",
    "@types/marked": "^3.0.1",
    "@types/node": "^16.3.2",
    "@types/prismjs": "^1.16.6",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "autoprefixer": "^10.3.4",
    "clean-css": "^5.1.5",
    "eslint": "~7.26.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.1",
