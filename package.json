{
  "name": "ngaox",
  "version": "2.1.0",
  "scripts": {
    "ng": "nx",
    "nx-all": "nx run-many --all --exclude=docs-functions --target",
    "nx-effected": "nx affected --target",
    "start": "nx serve",
    "build": "nx build",
    "deploy": "nx deploy",
    "lint": "nx lint",
    "test": "nx test",
    "watch": "nx build --watch --c development",
    "docs:deploy": "nx run-many --target=deploy --projects=docs-app,docs-functions",
    "docs:start": "npm-run-all -p \"watch docs-app\" \"watch docs-functions\" docs:emulator:start",
    "docs:emulator:start": "firebase emulators:start --only=functions,hosting",
    "prepare": "husky install tools/.husky",
    "postinstall": "node ./tools/decorate-angular-cli.js && ngcc --properties es2015 browser module main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/fire": "^7.2.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "firebase": "^9.6.1",
    "firebase-admin": "^10.0.1",
    "firebase-functions": "^3.16.0",
    "marked": "^4.0.10",
    "prismjs": "^1.25.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@nrwl/angular": "13.4.1",
    "@nrwl/cli": "13.4.1",
    "@nrwl/eslint-plugin-nx": "13.4.1",
    "@nrwl/jest": "13.4.1",
    "@nrwl/js": "13.4.1",
    "@nrwl/linter": "13.4.1",
    "@nrwl/node": "^13.4.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "13.4.1",
    "@nrwl/workspace": "13.4.1",
    "@types/jest": "27.0.2",
    "@types/marked": "^3.0.1",
    "@types/node": "14.14.33",
    "@types/prismjs": "^1.16.6",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "lint-staged": "^12.1.4",
    "ng-packagr": "^13.0.0",
    "ngx-deploy-npm": "^3.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3",
    "yargs": "^17.1.1"
  },
  "lint-staged": {
    "package.json": [
      "node ./tools/sync-versions.js",
      "git add"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,json,css,scss,md,ts,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
